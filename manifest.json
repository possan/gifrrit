{
  "name": "Gifrrit",
  "description": "Animated code reviews",
  "version": "2.0",
  "permissions": [
    "activeTab"
  ],
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "/jquery.js",
        "/content.js"
      ]
    }
  ]
}
