{
  "name": "Gifrrit",
  "description": "Animated code reviews",
  "icons": {
    "256": "icon256.png",
    "128": "icon128.png",
    "64": "icon64.png"
  },
  "version": "1.0",
  "permissions": [
    "activeTab"
  ],
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "/jquery.js",
        "/content.js"
      ]
    }
  ]
}
